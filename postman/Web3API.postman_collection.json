	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"let body = `{\"username\": \"${pm.variables.get('username')}\",",
					"            \"password\": \"${pm.variables.get('password')}\"}`",
					"    console.log(body);",
					"",
					"            const sendrequest = {",
					"                url: pm.variables.get(\"url\")+ \"/api/v1/user/authenticate\",",
					"                method: \"POST\",",
					"                header: {",
					"                    'Content-Type': 'application/json',",
					"                    'Accept': '*/*'",
					"                    ",
					"                },",
					"                body: body",
					"            }",
					"    pm.sendRequest(sendrequest, function (err, response) {",
					"            ",
					"        try{",
					"            pm.expect(response.json().token).to.not.empty;",
					"            pm.variables.set('token', response.json().token);",
					"        } ",
					"        catch(e){",
					"            console.log(e);",
					"        }",
					"    })"
				]
			}
		},
